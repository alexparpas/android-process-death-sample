<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alexparpas.sample.process">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".troublemakers.stateful.TroublemakerStatefulActivity" />
        <activity android:name=".troublemakers.singleton.TroublemakerSingletonActivity" />
        <activity android:name=".examples.recycler.RecyclerViewExampleActivity" />
        <activity android:name=".examples.navigation.NavigationExampleActivity" />
        <activity android:name=".examples.basic.BasicExampleActivity" />
    </application>

</manifest>